@model V2boardApi.Areas.App.Data.SubscriptionsViewModels.GetUserDataModel
@{
    Layout = "~/Areas/api/Views/Shared/_LayoutSubPage.cshtml";

}

@section Style{
    <link href="~/assets/SubPage/Home/css2.css" rel="stylesheet">
    <link rel="stylesheet" href="~/assets/SubPage/Home/index-7a16ca74.css">

}

<div class="max-w-[450px] mx-auto py-4 px-4 bg-white dark:bg-neutral-900 h-full flex-grow w-full">
    <div class="relative flex items-end"></div>
    <div class="py-8">
        <div class="w-full flex items-center flex-col">
            <div class="rounded-full border-2 border-primary dark:border-primary-dark bg dark:bg-primary-dark-fade dark:bg-zinc-800 text-primary dark:text-primary-dark p-6 w-fit relative">
                <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1px"
                     stroke="currentColor" aria-hidden="true" width="60px">
                    <path stroke-linecap="round" stroke-linejoin="round"
                          d="M15.75 6a3.75 3.75 0 11-7.5 0 3.75 3.75 0 017.5 0zM4.501 20.118a7.5 7.5 0 0114.998 0A17.933 17.933 0 0112 21.75c-2.676 0-5.216-.584-7.499-1.632z">
                    </path>
                </svg><span class="text-white whitespace-nowrap text-xs text-center inline-block absolute -bottom-3 py-[3px] px-3 rounded-md border-2 border-white dark:border-zinc-900 left-1/2 -translate-x-1/2 bg-amber-500 dark:bg-amber-600">
                    @Model.Name.Split('@')[0].Split('$')[0]
                </span>
            </div>
            <div class="flex justify-center w-full mt-5">
                @if (Model.IsActive == 1)
                {
                    <span class="text-slate-800 text-xs font-bold px-2 py-1 rounded-sm bg-green shadow-md">
                        فعال
                    </span>
                }
                else if (Model.IsActive == 2 || Model.IsActive == 3 || Model.IsActive == 4)
                {
                    <span class="text-slate-800 text-xs font-bold px-2 py-1 rounded-sm bg-red-500 shadow-md">
                        اتمام تاریخ انقضا
                    </span>
                }

            </div>
            <div class="flex flex-col max-w-[230px] w-full py-4 mt-2 font-semibold gap-2">
                <div class="flex justify-between w-full">
                    <span class="text-gray dark:text-slate-200">حجم کل</span><span class="text-primary dark:text-primary-dark" dir="ltr">@Model.TotalVolume</span>
                </div>
                <div class="flex justify-between w-full">
                    <span class="text-gray dark:text-slate-200">
                        حجم مصرف
                        شده
                    </span><span class="text-primary dark:text-primary-dark" dir="ltr">@Model.UsedVolume</span>
                </div>
                <div class="flex justify-between w-full">
                    <span class="text-gray dark:text-slate-200">
                        حجم باقیمانده
                    </span><span class="text-primary dark:text-primary-dark" dir="ltr">@Model.RemainingVolume</span>
                </div>
                <div class="flex justify-between w-full">
                    <span class="text-gray dark:text-slate-200">
                        انقضا
                    </span>
                    @if (Model.ExpireDate == null)
                    {
                        <span class="text-primary dark:text-primary-dark" dir="rtl">ندارد</span>
                    }
                    else
                    {
                        <span class="text-primary dark:text-primary-dark" dir="rtl">@Model.ExpireDate</span>
                    }
                </div>
            </div>
            <hr class="border-black dark:border-slate-300 opacity-25 mx-auto w-1/2 my-4 mt-7">
            <div class="flex flex-col justify-center text-center gap-2 py-5 font-semibold text-black dark:text-slate-300">
